                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/verticalSeekBarWrapper8"
                android:layout_toRightOf="@+id/verticalSeekBarWrapper8"
                android:max="100"
                android:progress="50"
                app:seekBarRotation="CW270" />
        </com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper>

        <com.h6ah4i.android.widget.verticalseekbar.VerticalSeekBarWrapper
            android:id="@+id/verticalSeekBarWrapper6"
            android:layout_width="48dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/verticalSeekBarWrapper5"
            android:layout_toRightOf="@+id/verticalSeekBarWrapper5"
